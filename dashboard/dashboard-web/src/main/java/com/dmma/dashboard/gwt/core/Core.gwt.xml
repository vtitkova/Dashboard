<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC 
  	"-//Google Inc.//DTD Google Web Toolkit 2.0.3//EN" 
    "http://google-web-toolkit.googlecode.com/svn/tags/2.0.3/distro-source/core/src/gwt-module.dtd">
<module rename-to='Core'>
	<inherits name='com.google.gwt.user.User' />
	<inherits name='com.google.gwt.user.theme.standard.Standard' />
	<inherits name="com.google.gwt.i18n.I18N"/>
	<extend-property name="locale" values="en"/>
	<extend-property name="locale" values="no"/>
	
	<inherits name='gwt.dmma.base.Base' />
	<inherits name='com.dmma.base.gwt.Base' />
	<source path="client"/>
	<source path="../../base/gwt/client"/>
	<source path='shared'/>
</module>