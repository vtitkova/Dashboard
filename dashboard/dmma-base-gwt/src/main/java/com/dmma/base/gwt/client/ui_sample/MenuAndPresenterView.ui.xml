<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>

	<ui:style src="MenuAndPresenterSample.css"/>
	
	<g:HTMLPanel>
		<table class="{style.mapMainContentTable}" cellpadding="0" cellspacing="0">
			<tr>
				<td width="220px" class="{style.menuPanelHolder}">
					<g:StackLayoutPanel ui:field="menuPanel" width="100%"></g:StackLayoutPanel> 
				</td>
				<td class="{style.workAreaPanelHolder}">
					<g:FlowPanel ui:field="workAreaPanel" styleName="{style.workAreaPanel}"></g:FlowPanel>
				</td>
			</tr>
		</table>
	</g:HTMLPanel>
</ui:UiBinder>