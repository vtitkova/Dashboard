<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>

	<ui:style src="MenuAndPresenterSample.css"/>
	
	<g:HTMLPanel styleName="{style.presenterView}">
		<table class="{style.presenterTable}" cellpadding="0" cellspacing="0">
			<tr>
				<td class="{style.presenterTableTH}">
					<g:HTML ui:field="captionHT" styleName="{style.captionHT}"></g:HTML>
				</td>
			</tr>
			<tr>
				<td>
					<g:FlowPanel ui:field="contentPanel" width="100%"></g:FlowPanel>
				</td>
			</tr>
		</table>
	</g:HTMLPanel>
</ui:UiBinder>